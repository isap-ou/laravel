{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The skeleton application for the Laravel framework.",
  "keywords": [
    "laravel",
    "framework"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "althinect/filament-spatie-roles-permissions": "^2.2",
    "archilex/filament-toggle-icon-column": "^3.1",
    "artesaos/seotools": "^1",
    "bezhansalleh/filament-google-analytics": "^2.0",
    "bezhansalleh/filament-language-switch": "^3.1",
    "codezero/laravel-unique-translation": "^4.3",
    "filament/filament": "^3.2",
    "filament/spatie-laravel-media-library-plugin": "^3.2",
    "guava/filament-nested-resources": "^1.0",
    "inertiajs/inertia-laravel": "^1",
    "joaopaulolndev/filament-edit-profile": "^1.0",
    "joaopaulolndev/filament-general-settings": "^1.0",
    "laravel/framework": "^11.9",
    "laravel/horizon": "^5",
    "laravel/tinker": "^2.9",
    "mcamara/laravel-localization": "^2.0",
    "njxqlus/filament-progressbar": "^1.0",
    "sentry/sentry-laravel": "^4.4",
    "shuvroroy/filament-spatie-laravel-backup": "^2.1",
    "shuvroroy/filament-spatie-laravel-health": "^2.0",
    "spatie/laravel-permission": "^6",
    "statikbe/laravel-cookie-consent": "^1",
    "statikbe/laravel-filament-chained-translation-manager": "^3.2",
    "statikbe/laravel-filament-flexible-content-blocks": "^2.0",
    "tomatophp/filament-users": "^1.0",
    "valentin-morice/filament-json-column": "^1.3",
    "ycs77/inertia-laravel-ssr-head": "^1.3",
    "ysfkaya/filament-phone-input": "^2.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "barryvdh/laravel-debugbar": "^3",
    "barryvdh/laravel-ide-helper": "^3.0",
    "fakerphp/faker": "^1.23",
    "laravel-lang/common": "^6.2",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0.1",
    "spatie/laravel-ignition": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
      "@php artisan filament:upgrade"
    ],
    "post-install-cmd": [
      "@php artisan permissions:sync"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan ide-helper:generate",
      "@php artisan ide-helper:meta",
      "@php artisan permissions:sync"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
